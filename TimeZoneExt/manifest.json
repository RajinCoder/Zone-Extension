{
    "name": "Zone",
    "description": "A Simple Time Zone Extension",
    "author": "Peter Moise",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "16": "Icons/clock-16.png",
        "32": "Icons/clock-32.png",
        "48": "Icons/clock-48.png",
        "128": "Icons/clock-128.png"
        }
    },
    "icons": {
        "16": "Icons/clock-16.png",
        "32": "Icons/clock-32.png",
        "48": "Icons/clock-48.png",
        "128": "Icons/clock-128.png"
    },
    "permissions": ["storage"]
}